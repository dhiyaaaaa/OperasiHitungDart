import 'dart:math';  // Untuk fungsi matematika sqrt()

void main() {
  // Memasukkan angka untuk diketahui akar kuadratnya
  double number = 25;

  // Pengecekan bilangan negatif
  if (number < 0) {
    print('Akar kuadrat tidak dapat dihitung untuk bilangan negatif.');
  } else {
    // Menghitung akar kuadrat
    double result = sqrt(number);

    // Membulatkan hasil ke 2 angka desimal
    String roundedResult = result.toStringAsFixed(2);

    // Menampilkan hasil
    print('Akar kuadrat dari $number adalah $roundedResult');
  }
}
